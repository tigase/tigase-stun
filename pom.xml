
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
        <modelVersion>4.0.0</modelVersion>
        <groupId>tigase</groupId>
        <artifactId>tigase-stun</artifactId>
        <name>Tigase STUN Component</name>
        <version>1.1.0-SNAPSHOT</version>
        <description>STUN server component for Tigase XMPP Server</description>
        <url>https://projects.tigase.org/projects/stun</url>
        <organization>
                <name>Tigase</name>
                <url>http://tigase.org</url>
        </organization>
        <build>
                <plugins>
                        <plugin>
                                <artifactId>maven-compiler-plugin</artifactId>
                                <configuration>
                                        <source>1.6</source>
                                        <target>1.6</target>
                                        <encoding>UTF-8</encoding>
                                </configuration>
                        </plugin>
                        <plugin>
                                <artifactId>maven-pmd-plugin</artifactId>
                        </plugin>
                </plugins>
        </build>
        <dependencies>
                <dependency>
                        <groupId>tigase</groupId>
                        <artifactId>tigase-server</artifactId>
                        <version>5.1.0-SNAPSHOT</version>
                        <scope>compile</scope>
                </dependency>
                <dependency>
                        <groupId>de.javawi</groupId>
                        <artifactId>jstun</artifactId>
                        <version>0.7.3</version>
                        <scope>compile</scope>
                </dependency>
        </dependencies>
</project>
